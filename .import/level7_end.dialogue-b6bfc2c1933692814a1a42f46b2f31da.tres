[sub_resource type="Resource" id=10]
script = ExtResource( 1 )
title = " celes"
message = "So what, a lady can't have a guilty pleasure? I'm 67, I'll eat what I want! Anyway. She lied to you about the Crystal, didn't she? And she probably told you some wild story about us having massive weapons too didn't she?
"
music = ""

[sub_resource type="Resource" id=11]
script = ExtResource( 1 )
title = " kris"
message = "Yes! I mean... No!! Wha... What is up with that giant Crystal shrine anyway? It looks really suspicious!
"
