[sub_resource type="Resource" id=7]
script = ExtResource( 1 )
title = " celes"
message = "Are you serious? You don't know what Elyne has been doing to us? She's been trying to conquer us for decades! She's the one who cast a spell on the cute little bobas to become hostile! I'm just trying to defend our people!
"
